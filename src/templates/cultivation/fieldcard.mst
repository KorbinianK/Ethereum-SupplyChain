<div class="card text-white bg-primary">
    <img class="card-img-top" src="{{image}}" alt="{{image_alt}}">
    <div class="card-body">
        <h5 class="card-title">{{field_name}}</h5>
        <p class="card-text">{{field_details}}</p>
        <p class="card-text"><small class="text-white">{{field_address}}</small></p>
    </div>
     <button class="text-white btn btn-primary btn-block changeFieldName" data-address="{{field_address}}" type="button">Change Name</button>
</div>